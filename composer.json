{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.2.*",
		"zizaco/entrust": "1.2.*",
		"h4cc/wkhtmltopdf-amd64": "0.12.x",
		"h4cc/wkhtmltoimage-i386": "0.12.x",
	  	"nathanmac/laravel-restclient": "dev-master",
		"picqer/php-barcode-generator": "^0.2.0",
		"maatwebsite/excel": "~1.2.1"
	},
	"require-dev":{
		"phpunit/phpunit": "4.2.*",
		"mockery/mockery": "0.9.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/api"
			
		],
		"psr-4": {
			"KBLIS\\Instrumentation\\": "app/kblis/instrumentation",
			"KBLIS\\Plugins\\": "app/kblis/plugins",
			"Nlims\\Service\\": "app/Nlims/Service"

		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
